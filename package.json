{
  "name": "prisma-article-example",
  "description": "&lt;&lt;Prisma: 下一代ORM, 不仅仅是ORM>> Demo 仓库",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run compile && npm run init",
    "compile": "rimraf -rf dist && etsc && node copy.js",
    "init": "npm run init:single && npm run init:multi",
    "init:single": "cd dist/single-model && prisma db push --preview-feature && prisma generate",
    "init:multi": "cd dist/multi-models && prisma db push --preview-feature && prisma generate",
    "invoke": "npm run invoke:single && npm run invoke:multi",
    "invoke:single": "node dist/single-model/index.js",
    "invoke:multi": "node dist/multi-models/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linbudu599/Prisma-Article-Example.git"
  },
  "author": "linbudu599",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linbudu599/Prisma-Article-Example/issues"
  },
  "homepage": "https://github.com/linbudu599/Prisma-Article-Example#readme",
  "dependencies": {
    "@prisma/cli": "^2.14.0",
    "@prisma/client": "^2.14.0",
    "apollo-server": "^2.19.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.1",
    "type-graphql": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "chalk": "^4.1.0",
    "esbuild-node-tsc": "^1.0.5",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.7",
    "ora": "^5.2.0",
    "ts-node": "^9.1.1"
  }
}
